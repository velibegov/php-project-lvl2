{
  "name": "hexlet/code",
  "description": "difference analyzer",
  "type": "project",
  "require": {
    "docopt/docopt": "^1.0",
    "ext-json": "*",
    "symfony/yaml": "3.4.x-dev",
    "tightenco/collect": "8.34.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Ruslan",
      "email": "velibegov@mail.ru"
    }
  ],
  "bin": [
    "bin/gendiff"
  ],
  "scripts": {
    "phpcs": "./vendor/squizlabs/php_codesniffer/bin/phpcs",
    "test": "phpunit"
  },
  "autoload": {
    "psr-4": {
      "Differ\\Differ\\": "src/",
      "Tests\\": "tests/"
    },
    "files": [
      "src/Parsers.php",
      "src/Formatters/Stylish.php",
      "src/Formatters/Plain.php",
      "src/Formatters/Json.php",
      "src/DiffBuilder.php",
      "src/Formatter.php"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "phpstan/phpstan": "^0.12.79",
    "squizlabs/php_codesniffer": "3.*"
  }
}
